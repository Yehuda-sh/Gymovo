{
  "expo": {
    "name": "Gymovo",
    "slug": "gymovo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/branding/app-icon.png",
    "scheme": "gymovo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/branding/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gymovo.app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["gymovo"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/branding/app-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.gymovo.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "gymovo",
              "host": "auth"
            },
            {
              "scheme": "https",
              "host": "*.supabase.co",
              "pathPrefix": "/auth/v1/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/branding/app-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/branding/logo.png",
          "backgroundColor": "#000000",
          "resizeMode": "contain"
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
